<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/searchImageFragment">

    <fragment
        android:id="@+id/searchImageFragment"
        android:name="com.example.searchimageara.ui.searchimage.SearchImageFragment"
        android:label="fragment_image_search"
        tools:layout="@layout/fragment_image_search" >

        <action
            android:id="@+id/action_searchImageFragment_to_imageDetailDialog2"
            app:destination="@id/imageDetailDialog2" />
        <action
            android:id="@+id/action_searchImageFragment_to_imageDataWebSite"
            app:destination="@id/imageDataWebSite" />
    </fragment>
    <dialog
        android:id="@+id/imageDetailDialog2"
        android:name="com.example.searchimageara.ui.imagedetail.ImageDetailDialogFragment"
        android:label="ImageDetailDialog" />
    <fragment
        android:id="@+id/imageDataWebSite"
        android:name="com.example.searchimageara.ui.webpage.ImageDataWebSite"
        android:label="ImageDataWebSite" />

</navigation>